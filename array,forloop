pragma solidity ^0.4.0;
contract bank
{           uint256[] arr;
                uint256[] ar;
                uint256 s;
                //uint256 temp=n;

    function b(uint256 n,uint256 m)public constant returns(uint256[],uint256[]){ 
        s=n;
        for( n=0;n<=m;n++){
            arr.push(n);
        
        }
             for(uint256 i=s;i<=s;i--){
                 ar.push(i);
             }return(arr,ar);
        }
    }
